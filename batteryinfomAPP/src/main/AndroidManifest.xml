<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.he.batteryinfoActivity">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.he.batteryinfoActivity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.he.batteryinfoActivity.TableOneActivity" />
        <activity android:name="com.example.he.batteryinfoActivity.TableTwoActivity" />
        <activity android:name="com.example.he.batteryinfoActivity.LineChartActivity"
            android:screenOrientation="landscape"/>
        <activity android:name="com.example.he.batteryinfoActivity.MultiLineChartActivity"
            android:screenOrientation="landscape"/>
    </application>

</manifest>
